{
    "name": "tiktok-live-interactive",
    "version": "1.0.0",
    "description": "Sistema interativo para lives do TikTok com mensagens em tempo real",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server.js",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "install-all": "npm install && cd client && npm install"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "qrcode": "^1.5.3",
        "socket.io": "^4.7.2",
        "socket.io-client": "^4.8.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "concurrently": "^8.2.0",
        "nodemon": "^3.0.1"
    },
    "keywords": [
        "tiktok",
        "live",
        "interactive",
        "messages",
        "realtime"
    ],
    "author": "Seu Nome",
    "license": "MIT"
}
